{
    "name": "@acctos-ai/root",
    "version": "1.0.0",
    "private": true,
    "workspaces": [
        "apps/*",
        "packages/*"
    ],
    "scripts": {
        "dev": "concurrently \"npm run dev:api\" \"npm run dev:web\"",
        "dev:api": "npm run dev -w apps/api",
        "dev:web": "npm run dev -w apps/web",
        "build": "npm run build -w apps/api && npm run build -w apps/web",
        "db:generate": "npm run db:generate -w apps/api",
        "db:push": "npm run db:push -w apps/api",
        "db:migrate": "npm run db:migrate -w apps/api",
        "db:studio": "npm run db:studio -w apps/api",
        "docker:up": "docker-compose up -d",
        "docker:down": "docker-compose down",
        "start:project": "npm run docker:up && npm run db:push && npm run dev"
    },
    "devDependencies": {
        "concurrently": "^8.2.2",
        "typescript": "^5.3.3"
    }
}